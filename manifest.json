{
    "manifest_version": 2,
    "name": "Tap Tab",
    "description": "Simple way to view and manage your open tabs",
    "version": "0.1",
    "permissions": [
        "tabs"
    ],
    "icons":{
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
