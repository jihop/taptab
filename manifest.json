{
    "manifest_version": 2,
    "name": "Taptab",
    "description": "Simple tab manager for chrome",
    "version": "0.3",
    "permissions": [
        "tabs"
    ],
    "icons":{
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+S"
            }
        }
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
